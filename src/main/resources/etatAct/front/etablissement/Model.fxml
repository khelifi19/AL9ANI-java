<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane stylesheets="@../../MainStyle.css" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane layoutX="30.0" layoutY="30.0" style="-fx-background-color: white;" styleClass="window-rounded"
                AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"
                AnchorPane.topAnchor="30.0">

        <HBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">

            <VBox spacing="10.0">
                <HBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </HBox.margin>
                <Text fx:id="nomText" text="Nom" wrappingWidth="190">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>
                <Text fx:id="adresseText" text="Adresse" wrappingWidth="190">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>
                <Text fx:id="telephoneText" text="Telephone" wrappingWidth="190">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>


            </VBox>
            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </HBox.margin>
                <Text fx:id="categorieText" text="Categorie" wrappingWidth="190">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>
                <Text fx:id="emailText" text="Email" wrappingWidth="190">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>

                <Text fx:id="userText" text="User" wrappingWidth="190">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>

            </VBox>

        </HBox>
    </AnchorPane>
</AnchorPane>
